{
  "name": "niem-specification-utils",
  "version": "1.0.0",
  "description": "Extracts rule and definition information from NIEM specifications.",
  "main": "./index.js",
  "bin": {
    "cli": "bin/index.js"
  },
  "scripts": {
    "build": "npm run clean && node bin/index.js",
    "build:debug": "cross-env DEBUG=niem npm run build",
    "ci": "npm run test:cov && npm run coverage && npm run docs",
    "clean": "trash output && trash test/output",
    "coverage": "coveralls < coverage/lcov.info",
    "docs": "trash docs && jsdoc . ./src/index.js ./src ./README.md -d docs -t node_modules/docdash",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.0.0-rc.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.1",
    "fs-extra": "^9.1.0",
    "trash": "^7.1.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "coveralls": "^3.1.0",
    "docdash": "^1.2.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "trash-cli": "^4.0.0"
  }
}
